SELECT 
    CASE 
        WHEN (exudate IS NULL OR exudate = '') THEN
            CASE 
                WHEN disamtsqtatus = 'E' THEN 'I'
                ELSE 'F'
            END
        ELSE
            CASE 
                WHEN disamtsqtatus = 'E' THEN 'H'
                WHEN disamtsqtatus = 'T' THEN 'G'
                ELSE 'A'
            END
    END AS recTYP
FROM your_table_name;
